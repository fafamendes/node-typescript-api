{
  "name": "node-typescript-api",
  "version": "0.0.1",
  "description": "A simple node.js api using typescript, express, mongodb and jest",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev ./src/index.ts",
    "test:functional": "jest --projects ./test --forceExit --runInBand --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fafamendes/node-typescript-api.git"
  },
  "keywords": [
    "api",
    "typescript",
    "node.js",
    "mongodb",
    "jest"
  ],
  "author": "Fabr√≠cio Mendes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fafamendes/node-typescript-api/issues"
  },
  "homepage": "https://github.com/fafamendes/node-typescript-api#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/json-schema": "^7.0.12",
    "@types/module-alias": "^2.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.5.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "mongoose": "^7.5.0"
  }
}
